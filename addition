#import <Foundation/Foundation.h>

int main(int argc, const char * argv[]) {
    @autoreleasepool {
        
       int a, b, sum;
       NSlog(@"Enter first value: ");
       scanf("%d", a);
       NSlog(@"Enter second value: ");
       scanf("%d", b);
       sum=a+b;
       NSLog(@"%d", sum);
    }
    return 0;
}
